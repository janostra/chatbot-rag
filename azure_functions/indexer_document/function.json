{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "documents/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputDocument",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "chatbot",
      "collectionName": "documents",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}